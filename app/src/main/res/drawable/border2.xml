<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#e9eff1" />
            <solid android:color="#fff" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width=".5dp"
                android:color="#e9eff1" />
        </shape>
    </item>

</layer-list>